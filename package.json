{
  "name": "remark-excalidraw",
  "version": "1.0.0",
  "description": "To render the excalidraw on website iframe",
  "main": "index.js",
  "keywords": [
    "excalidraw",
    "render"
  ],
  "author": "fwx5618177",
  "license": "ISC",
  "repository": {
    "url": ""
  },
  "scripts": {
    "postinstall": "npm run build:prod",
    "prepare": "husky install",
    "test": "mocha",
    "test:watch": "mocha --watch --clean-cache",
    "start": "node --import tsx dist/main.js",
    "pub:beta": "npm version prerelease --preid=beta",
    "pub:patch": "npm version patch",
    "rm": "rimraf dist",
    "build": "swc src -d dist --source-maps -D --strip-leading-paths",
    "build:type": "tsc --emitDeclarationOnly",
    "build:prod": "npm run rm && npm run build:type && npm run build",
    "lint": "eslint --config .eslintrc . --cache --debug",
    "lint:fix": "eslint --fix .",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.{ts,js}\""
  },
  "engines": {
    "node": ">=20.10.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.14",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.7",
    "@types/unist": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "chai": "^5.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "mocha": "^10.4.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "unified": "^11.0.4"
  },
  "dependencies": {
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.4.14",
    "@types/react": "^18.2.78",
    "@types/react-dom": "^18.2.25",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remark": "^15.0.1",
    "remark-parse": "^11.0.0",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "unist-builder": "^4.0.0",
    "unist-util-is": "^6.0.0",
    "unist-util-visit": "^5.0.0"
  }
}