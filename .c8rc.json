{
    "reporter": [
        "lcov",
        "text"
    ],
    "exclude": [
        "node_modules",
        "bower_components",
        "tmp",
        "vendor",
        "dist",
        "coverage",
        "testem.log",
        "tests",
        "**/*.spec.ts",
        "**/*.test.ts"
    ],
    "include": [
        "src/**/*.ts"
    ],
    "extensions": [
        "ts"
    ],
    "all": true
}